	"RctExponentialGrowth":{
		"caption":"Exponential growth",
		"content":[
			{
				"title": "Exponential growth",
				"content": [
					["{`\\rm{ x = x_0 e^{kt} }`}"],
					["{`\\rm{ x = }`}"," quantity after elapsed time"],
					["{`\\rm{ x_0 = }`}"," initial quantity"],
					["{`\\rm{ k = }`}"," instantaneous growth constant (1/s)"],
					["{`\\rm{ t = }`}"," elapsed time (s)"]
				],
				"subsections": [
					{
						"title": "Generation time",
						"content": [
							["{`\\rm{ g = \\frac{t}{n} = \\frac{ \\ln{2} }{ k } }`}"],
							["{`\\rm{ g = }`}"," elapsed time per generation (s)"],
							["{`\\rm{ t = }`}"," elapsed time (s)"],
							["{`\\rm{ n = }`}"," quantity of generations after elapsed time"],
							["{`\\rm{ k = }`}"," instantaneous growth constant (1/s)"]
						],
						"subsections": []
					},{
						"title": "Example",
						"content": [
							["If the generation time of particular bacterium is 20 minutes, then how many generations will have occured after 1 day?"],
							["{`\\rm{ g = 20 \\text{ minutes} }`}"],
							["{`\\rm{ t = 1 \\text{ day} }`}"],
							["{`\\rm{ g = \\frac{t}{n} \\longrightarrow n = \\frac{t}{g} }`}"],
							["{`\\rm{ n = 1 \\text{ day} \\left(\\frac{1 \\text{ generation}}{20 \\text{ minutes}} \\right) \\left( \\frac{ 24 \\text{ hours} }{ 1 \\text{ day} }\\right) \\left( \\frac{ 60 \\text{ minutes} }{ 1 \\text{ hour} }\\right) }`}"],
							["{`\\rm{ n = 72 \\text{ generations} }`}"],
							["If the initial population is 5, what will be the population after 36 hours?"],
							["{`\\rm{ x_0 = 5 \\text{ cells} }`}"],
							["{`\\rm{ t = 36 \\text{ hours} }`}"],
							["{`\\rm{ g = \\frac{ \\ln{2} }{ k } \\longrightarrow k = \\frac{ \\ln{2} }{ g } }`}"],
							["{`\\rm{ k = \\frac{ \\ln{2} }{ 20 \\text{ minutes} }\\left(\\frac{60 \\text{ minutes}}{1 \\text{ hour}}\\right) }`}"],
							["{`\\rm{ k = 2.08 \\text{ hour}^{-1} }`}"],
							["{`\\rm{ x = x_0 e^{kt} }`}"],
							["{`\\rm{ x = \\left( 5 \\text{ cells}\\right) e^{\\left(\\frac{2.08}{\\text{hour}}\\right)36 \\text{ hours}} = 1.623\\times10^{33}\\text{ cells}}`}"]
						],
						"subsections": []
					}
				]
			}
		]
	},
	"RctMicrobeCmfr":{
		"caption":"Microbe CMFR",
		"content":[
			{
				"title": "Microbe CMFR",
				"content": [],
				"subsections": [
					{
						"title": "Flow",
						"content": [
							["{`\\rm{ Q = Q_{ in } = Q_{ out } }`}"],
							["{`\\rm{ Q = }`}"," flow"],
							["{`\\rm{ Q_{ in } = }`}"," flow into the reactor"],
							["{`\\rm{ Q_{ out } = }`}"," flow out of the reactor"]
						],
						"subsections": []
					},{
						"title": "Microbial mass balance",
						"content": [
							["{`\\rm{ \\frac{dX}{dt}V = QX_0 - QX + r_{net}V }`}"],
							["{`\\rm{ r_{net} = \\frac{ \\mu_{max}S_X }{ K_S + S } - bX }`}"],
							["If stead-state, then: ","{`\\rm{ \\frac{dX}{dt} = 0 }`}"],
							["If ","{`\\rm{ \\frac{dX}{dt} = 0 }`}"," and ","{`\\rm{ X_0 }`}"," is negligible, then:"],
							["{`\\rm{ r_{net} = \\frac{QX}{V} = \\frac{X}{\\theta} }`}"],
							["{`\\rm{ \\frac{\\mu_{max}S}{k_g+S}-b = \\frac{r_{net}}{X} = \\frac{1}{\\theta} }`}"]
						],
						"subsections": []
					},{
						"title": "Substrate mass balance",
						"content": [
							["{`\\rm{ \\frac{dS}{dt}V = QS_0 - QS + r_{ut}V }`}"],
							["If stead-state, then: ","{`\\rm{ \\frac{dS}{dt} = 0 }`}"],
							["If in exponential phase:"],
							["{`\\rm{ r_{ut} = -\\frac{ \\mu_{max}SX }{ Y(K_S + S) } = -\\frac{1}{Y} \\frac{dX}{dt} }`}"],
							["If not in exponential phase:"],
							["{`\\rm{ r_{ut} = -\\frac{1}{Y} \\left( \\frac{ \\mu_{max}S }{ (K_S + S) } - b \\right) }`}"],
							["{`\\rm{ QS = QS_0 - \\frac{\\mu_{max}SXV}{Y(K_S+S)} }`}"],
							["{`\\rm{ X = \\frac{Y(S_0 - S)}{1 + \\theta b} }`}"]
						],
						"subsections": []
					}
				]
			}
		]
	},
