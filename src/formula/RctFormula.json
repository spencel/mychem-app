{
	"RctSuspendedGrowthProcess":{
		"caption":"Suspended growth process",
		"isHtml":false,
		"content":[
			{
				"title": "Suspended growth process",
				"content": [
						["choose a value for X, 2000 to 5000 ","{`\\rm{\\left(mg_{VSS}/L\\right)}`}"]
					],
				"subsections": [
					{
						"title":"Activated sludge recycle",
						"content":[
							["{`\\rm{Q = Q_e + Q_w}`}"],
							["{`\\rm{V\\frac{dX}{dt} = QX_0 - Q_eX_e - Q_wX_w + r_{net}V}`}"],
							["{`\\rm{\\frac{dX}{dt}\\to 0, X_0 \\to 0, X_e\\to0}`}"],
							["{`\\rm{Q_wX_w = r_{net}V}`}"],
							["{`\\rm{V\\frac{dS}{dt} = QS_0 - Q_eS_e - Q_wS_w - r_{ut}V}`}"],
							["{`\\rm{\\frac{dS}{dt}\\to0}`}"],
							["{`\\rm{QS_0 = Q_eS_e + Q_wS_w + r_{ut}V}`}"]
						],
						"subsections":[]
					},{
						"title":"No activated sludge recycle",
						"content":[
							["{`\\rm{\\theta_c = \\theta}`}"],
							["{`\\rm{V\\frac{dX}{dt} = QX_0 - QX + r_{net}V}`}"],
							["{`\\rm{\\frac{dX}{dt}\\to 0, X_0 \\to 0}`}"],
							["{`\\rm{QX = r_{net}V}`}"],
							["{`\\rm{V\\frac{dS}{dt} = QS_0 - QS - r_{ut}V}`}"],
							["{`\\rm{\\frac{dS}{dt}\\to 0}`}"],
							["{`\\rm{QS_0 = QS + r_{ut}V}`}"]
						],
						"subsections":[]
					},{
						"title":"Shared equations",
						"content":[
							["{`\\rm{V=\\theta Q}`}"],
							["{`\\rm{S = S_e = S_w}`}"],
							["{`\\rm{S_{min} = \\frac{K_S b}{\\hat{\\mu} - b} =}`}"," minimum (effluent) substrate"],
							["{`\\rm{S = \\frac{K_S(1+b\\theta_c)}{\\theta_c(\\hat{\\mu}-b)-1}}`}"],
							["{`\\rm{X = \\frac{\\theta_c}{\\theta}\\left(\\frac{Y(S_0-S)}{1+\\theta_cb}\\right)}`}"],
							["{`\\rm{\\theta_{min} = \\frac{K_S + S_0}{S_0(\\hat{\\mu} - b) - K_S b} =}`}"," minimum time below which washout occurs"],
							["{`\\rm{[\\theta_{min}]_{lim} = \\frac{1}{\\hat{\\mu}-b} =}`}"," theoretical ultimate limit"],
							["{`\\rm{\\theta_c = [\\theta_{min}]_{lim} F_s = \\frac{X}{r_{net}} =}`}"," solids retention time (SRT) = mean cell residence time"],
							["{`\\rm{r_{net}=\\frac{\\hat{\\mu}X}{K_S+S}-bX}`}"],
							["{`\\rm{r_{ut}=\\frac{\\hat{\\mu}SX}{Y(K_S+S)} =}`}","utilization rate"],
							["{`\\rm{r_w = Q_w X_w = \\frac{XV}{\\theta_c} =}`}"," solids wasting rate ","{`\\rm{mg_{VSS}/d}`}"],
							["{`\\rm{Q_r:Q = \\frac{X}{X_w - X} =}`}"," recycle ratio"],
							["{`\\rm{F:M = \\frac{QS_0}{VX} = \\frac{S_0}{\\theta X} =}`}"," food to microbe ratio"],
							["{`\\rm{r_{O_2} = Q(S_0 - S) - 1.42r_w =}`}"," oxygen utilization rate, 1.42 portion of S used for biosynthesis"]
						],
						"subsections":[]
					},{
						"title":"Definitions",
						"content":[
							["{`\\rm{V = }`}"," volume of aeration basin (suspended growth reactor)"],
							["{`\\rm{F_s =}`}"," safety factor"],
							["{`\\rm{S_e = }`}"," effluent substrate or COD ","{`\\rm{\\left(\\frac{mg_{COD}}{L}\\right)}`}"],
							["{`\\rm{\\theta =}`}"," hydraulic retention time (HRT)"],
							["{`\\rm{X =}`}"," reactor mixed liquor suspended solids (MLSS) ","{`\\rm{\\left(\\frac{mg_{VSS}}{L}\\right)}`}"],
							["{`\\rm{S_0 = }`}"," influent substrate or COD ","{`\\rm{\\left(\\frac{mg_{COD}}{L}\\right)}`}"],
							["{`\\rm{K_S = }`}"," half-velocity constant ","{`\\rm{\\left(\\frac{mg_{COD}}{L}\\right)}`}"],
							["{`\\rm{X = }`}"," mixed liquor suspended solids (MLSS)"],
							["{`\\rm{Y = }`}"," true microbe yield ","{`\\rm{\\left(\\frac{mg_{VSS}}{mg_{COD}}\\right)}`}"," or ","{`\\rm{\\left(\\frac{mg_{cell}}{mg_{COD}}\\right)}`}"],
							["{`\\rm{Q_0 = }`}"," influent flow ","{`\\rm{\\left(\\frac{m^3}{h}\\right)}`}"],
							["{`\\rm{\\hat{\\mu} = }`}"," max microbe growth rate ","{`\\rm{\\left(\\frac{1}{h}\\right)}`}"],
							["{`\\rm{b = }`}"," microbe decay rate ","{`\\rm{\\left(\\frac{1}{h}\\right)}`}"]
						],
						"subsections":[]
					}
				]
			}
		]
	},
	"RctExponentialGrowth":{
		"caption":"Exponential growth",
		"isHtml":false,
		"content":[
			{
				"title": "Exponential growth",
				"content": [
					["{`\\rm{ x = x_0 e^{kt} }`}"],
					["{`\\rm{ x = }`}"," quantity after elapsed time"],
					["{`\\rm{ x_0 = }`}"," initial quantity"],
					["{`\\rm{ k = }`}"," instantaneous growth constant (1/s)"],
					["{`\\rm{ t = }`}"," elapsed time (s)"]
				],
				"subsections": [
					{
						"title": "Generation time",
						"content": [
							["{`\\rm{ g = \\frac{t}{n} = \\frac{ \\ln{2} }{ k } }`}"],
							["{`\\rm{ g = }`}"," elapsed time per generation (s)"],
							["{`\\rm{ t = }`}"," elapsed time (s)"],
							["{`\\rm{ n = }`}"," quantity of generations after elapsed time"],
							["{`\\rm{ k = }`}"," instantaneous growth constant (1/s)"]
						],
						"subsections": []
					},{
						"title": "Example",
						"content": [
							["If the generation time of particular bacterium is 20 minutes, then how many generations will have occured after 1 day?"],
							["{`\\rm{ g = 20 \\text{ minutes} }`}"],
							["{`\\rm{ t = 1 \\text{ day} }`}"],
							["{`\\rm{ g = \\frac{t}{n} \\longrightarrow n = \\frac{t}{g} }`}"],
							["{`\\rm{ n = 1 \\text{ day} \\left(\\frac{1 \\text{ generation}}{20 \\text{ minutes}} \\right) \\left( \\frac{ 24 \\text{ hours} }{ 1 \\text{ day} }\\right) \\left( \\frac{ 60 \\text{ minutes} }{ 1 \\text{ hour} }\\right) }`}"],
							["{`\\rm{ n = 72 \\text{ generations} }`}"],
							["If the initial population is 5, what will be the population after 36 hours?"],
							["{`\\rm{ x_0 = 5 \\text{ cells} }`}"],
							["{`\\rm{ t = 36 \\text{ hours} }`}"],
							["{`\\rm{ g = \\frac{ \\ln{2} }{ k } \\longrightarrow k = \\frac{ \\ln{2} }{ g } }`}"],
							["{`\\rm{ k = \\frac{ \\ln{2} }{ 20 \\text{ minutes} }\\left(\\frac{60 \\text{ minutes}}{1 \\text{ hour}}\\right) }`}"],
							["{`\\rm{ k = 2.08 \\text{ hour}^{-1} }`}"],
							["{`\\rm{ x = x_0 e^{kt} }`}"],
							["{`\\rm{ x = \\left( 5 \\text{ cells}\\right) e^{\\left(\\frac{2.08}{\\text{hour}}\\right)36 \\text{ hours}} = 1.623\\times10^{33}\\text{ cells}}`}"]
						],
						"subsections": []
					}
				]
			}
		]
	},
	"RctMicrobeCmfr":{
		"caption":"Microbe CMFR",
		"isHtml":false,
		"content":[
			{
				"title": "Microbe CMFR",
				"content": [],
				"subsections": [
					{
						"title": "Flow",
						"content": [
							["{`\\rm{ Q = Q_{ in } = Q_{ out } }`}"],
							["{`\\rm{ Q = }`}"," flow"],
							["{`\\rm{ Q_{ in } = }`}"," flow into the reactor"],
							["{`\\rm{ Q_{ out } = }`}"," flow out of the reactor"]
						],
						"subsections": []
					},{
						"title": "Microbial mass balance",
						"content": [
							["{`\\rm{ \\frac{dX}{dt}V = QX_0 - QX + r_{net}V }`}"],
							["{`\\rm{ r_{net} = \\frac{ \\mu_{max}S_X }{ K_S + S } - bX }`}"],
							["If stead-state, then: ","{`\\rm{ \\frac{dX}{dt} = 0 }`}"],
							["If ","{`\\rm{ \\frac{dX}{dt} = 0 }`}"," and ","{`\\rm{ X_0 }`}"," is negligible, then:"],
							["{`\\rm{ r_{net} = \\frac{QX}{V} = \\frac{X}{\\theta} }`}"],
							["{`\\rm{ \\frac{\\mu_{max}S}{k_g+S}-b = \\frac{r_{net}}{X} = \\frac{1}{\\theta} }`}"]
						],
						"subsections": []
					},{
						"title": "Substrate mass balance",
						"content": [
							["{`\\rm{ \\frac{dS}{dt}V = QS_0 - QS + r_{ut}V }`}"],
							["If stead-state, then: ","{`\\rm{ \\frac{dS}{dt} = 0 }`}"],
							["If in exponential phase:"],
							["{`\\rm{ r_{ut} = -\\frac{ \\mu_{max}SX }{ Y(K_S + S) } = -\\frac{1}{Y} \\frac{dX}{dt} }`}"],
							["If not in exponential phase:"],
							["{`\\rm{ r_{ut} = -\\frac{1}{Y} \\left( \\frac{ \\mu_{max}S }{ (K_S + S) } - b \\right) }`}"],
							["{`\\rm{ QS = QS_0 - \\frac{\\mu_{max}SXV}{Y(K_S+S)} }`}"],
							["{`\\rm{ X = \\frac{Y(S_0 - S)}{1 + \\theta b} }`}"]
						],
						"subsections": []
					}
				]
			},{
				"title":"Growth",
				"content":[
					["{`\\rm{\\frac{dX}{dt} = \\mu X - b X = \\frac{\\hat{\\mu} S}{K_S + S} - b X}`}"],
					["{`\\rm{\\frac{ dS }{ dt } = - \\frac{ 1 }{ Y } \\frac{ dX }{ dt }}`}"],
					["non-exponential growth: ","{`\\rm{\\frac{ dS }{ dt } = - \\frac{ 1 }{ Y }\\left(\\frac{\\hat{\\mu} S X}{K_S + S} - b X\\right)}`}"],
					["exponential growth: ","{`\\rm{\\frac{ dS }{ dt } = - \\frac{\\hat{\\mu} S X}{Y(K_S + S)}}`}"],
					["Y = g-biomass/g-substrate ","{`\\rm{\\left(\\frac{g_{VSS}}{g_{COD}}\\right)}`}"]
				],
				"subsections":[]
			}
		]
	},
	"RctMonodEquation":{
